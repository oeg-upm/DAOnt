<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="http://www.semanticweb.org/daont#"
     xml:base="http://www.semanticweb.org/daont"
     xmlns:dpv="https://w3c.github.io/dpv/2.2/dpv/#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:daont="http://www.semanticweb.org/daont#">

  <owl:Ontology rdf:about="http://www.semanticweb.org/daont/contracts/b2g-health">
    <owl:imports rdf:resource="http://www.semanticweb.org/daont"/>
    <rdfs:label>Public Health Emergency Data Sharing Contract (Violation)</rdfs:label>
    <rdfs:comment>
      ❌ VIOLATION scenario — Public authority used data to develop a competing product (Article 19(2)(a) violated).
    </rdfs:comment>
  </owl:Ontology>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#gonzalo">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ConsumerUser"/>
    <ownsOrUses rdf:resource="http://www.semanticweb.org/daont#healthMonitor1"/>
    <rdfs:label>Gonzalo</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#healthMonitor1">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ConnectedProduct"/>
    <generatesData rdf:resource="http://www.semanticweb.org/daont#gonzaloHealthData"/>
    <manufacturerIdentifier>HEALTH-MON-2024-GZ</manufacturerIdentifier>
    <productCategory>Medical Device</productCategory>
    <rdfs:label>Health Monitor 1</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#gonzaloHealthData">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ProductData"/>
    <rdf:type rdf:resource="https://w3c.github.io/dpv/2.2/dpv/#PersonalData"/>
    <isGeneratedBy rdf:resource="http://www.semanticweb.org/daont#healthMonitor1"/>
    <isMachineReadable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isMachineReadable>
    <isPersonal rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isPersonal>
    <hasDescription>Vital signs, disease indicators, health metrics for pandemic monitoring</hasDescription>
    <rdfs:label>Gonzalo Health Data</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#healthDeviceManufacturer">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#DataHolder"/>
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#Manufacturer"/>
    <dpv:hasData rdf:resource="http://www.semanticweb.org/daont#gonzaloHealthData"/>
    <dpv:hasRecipient rdf:resource="http://www.semanticweb.org/daont#healthAuthority"/>
    <rdfs:label>Health Device Manufacturer</rdfs:label>
  </owl:NamedIndividual>

  <!-- Public Authority performing prohibited action -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#healthAuthority">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#PublicSectorBody"/>
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#DataRecipient"/>
    <hasExceptionalNeedFor rdf:resource="http://www.semanticweb.org/daont#gonzaloHealthData"/>
    <daont:performsAction rdf:resource="http://www.semanticweb.org/daont#competitiveProductDevelopment1"/>
    <rdfs:label>Health Authority</rdfs:label>
  </owl:NamedIndividual>

  <!-- Prohibited Action -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#competitiveProductDevelopment1">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#UseDataToDevelopCompetingProduct"/>
    <hasDescription>Health authority used emergency data to develop a competing health app.</hasDescription>
    <rdfs:label>Competitive Product Development 1</rdfs:label>
    <rdfs:comment>Prohibited under Article 19(2)(a).</rdfs:comment>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#b2gSharingGonzalo">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#B2GDataSharing"/>
    <daont:governedBy rdf:resource="http://www.semanticweb.org/daont#contract191"/>
    <rdfs:label>B2G Sharing - Gonzalo</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#contract191">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#DataSharingAgreement"/>
    <dpv:hasRecipient rdf:resource="http://www.semanticweb.org/daont#healthAuthority"/>
    <agreementDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-03-20T09:00:00Z</agreementDate>
    <rdfs:label>Contract 191 - Public Health Emergency</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#publicHealthEmergency2024">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ExceptionalNeed"/>
    <hasDescription>Pandemic monitoring and public health response coordination</hasDescription>
    <rdfs:label>Public Health Emergency 2024</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#b2gDataRequest191">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#DataRequest"/>
    <respondsTo rdf:resource="http://www.semanticweb.org/daont#publicHealthEmergency2024"/>
    <hasDeadline rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-03-25</hasDeadline>
    <rdfs:label>B2G Data Request 191</rdfs:label>
  </owl:NamedIndividual>

</rdf:RDF>
