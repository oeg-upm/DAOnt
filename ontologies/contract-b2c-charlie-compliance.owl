<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="http://www.semanticweb.org/daont#"
     xml:base="http://www.semanticweb.org/daont"
     xmlns:dpv="https://w3c.github.io/dpv/2.2/dpv/#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:daont="http://www.semanticweb.org/daont#">

  <owl:Ontology rdf:about="http://www.semanticweb.org/daont/contracts/b2c-charlie">
    <owl:imports rdf:resource="http://www.semanticweb.org/daont"/>
    <rdfs:label>Charlie's Smart Watch Contract (Compliant)</rdfs:label>
    <rdfs:comment>
      ✅ COMPLIANT scenario — Manufacturer performs ProvideData action (Article 4(1)).
    </rdfs:comment>
  </owl:Ontology>

  <!-- Consumer -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#charlie">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ConsumerUser"/>
    <ownsOrUses rdf:resource="http://www.semanticweb.org/daont#smartWatch1"/>
    <requestsAccessTo rdf:resource="http://www.semanticweb.org/daont#charlieHealthData"/>
    <rdfs:label>Charlie</rdfs:label>
  </owl:NamedIndividual>

  <!-- Product -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#smartWatch1">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ConnectedProduct"/>
    <generatesData rdf:resource="http://www.semanticweb.org/daont#charlieHealthData"/>
    <manufacturerIdentifier>WATCH-2024-CH001</manufacturerIdentifier>
    <productCategory>Wearable Health Device</productCategory>
    <rdfs:label>Smart Watch 1</rdfs:label>
  </owl:NamedIndividual>

  <!-- Data -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#charlieHealthData">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ProductData"/>
    <rdf:type rdf:resource="https://w3c.github.io/dpv/2.2/dpv/#PersonalData"/>
    <isGeneratedBy rdf:resource="http://www.semanticweb.org/daont#smartWatch1"/>
    <isMachineReadable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isMachineReadable>
    <isPersonal rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isPersonal>
    <hasDescription>Heart rate, steps, sleep patterns, activity data</hasDescription>
    <rdfs:label>Charlie Health Data</rdfs:label>
  </owl:NamedIndividual>

  <!-- Data Holder -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#watchManufacturer">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#DataHolder"/>
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#Manufacturer"/>
    <dpv:hasData rdf:resource="http://www.semanticweb.org/daont#charlieHealthData"/>
    <performsLegalAction rdf:resource="http://www.semanticweb.org/daont#action_ProvideData_charlie"/>
    <rdfs:label>Watch Manufacturer</rdfs:label>
  </owl:NamedIndividual>

  <!-- B2C Data Sharing -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#b2cSharing_charlie">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#B2CDataSharing"/>
    <governedBy rdf:resource="http://www.semanticweb.org/daont#contract_charlie"/>
    <rdfs:label>B2C Sharing - Charlie</rdfs:label>
  </owl:NamedIndividual>

  <!-- Contract -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#contract_charlie">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#DataSharingAgreement"/>
    <dpv:hasRecipient rdf:resource="http://www.semanticweb.org/daont#charlie"/>
    <agreementDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-01-15T10:00:00Z</agreementDate>
    <rdfs:label>Charlie's Smart Watch Contract</rdfs:label>
  </owl:NamedIndividual>

  <!-- ProvideData Action -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#action_provideData_charlie">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#ProvideData"/>
    <hasProvider rdf:resource="http://www.semanticweb.org/daont#watchManufacturer"/>
    <hasRecipient rdf:resource="http://www.semanticweb.org/daont#charlie"/>
    <providesData rdf:resource="http://www.semanticweb.org/daont#charlieHealthData"/>
    <hasLegalBasis rdf:resource="http://www.semanticweb.org/daont#Article4_1_daont"/>
    <actionDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-01-20T09:00:00Z</actionDate>
    <rdfs:label>Provide Data to Charlie</rdfs:label>
  </owl:NamedIndividual>

  <!-- Legal Reference -->
  <owl:NamedIndividual rdf:about="http://www.semanticweb.org/daont#Article4_1_daont">
    <rdf:type rdf:resource="http://www.semanticweb.org/daont#LegalReference"/>
    <hasLegalSource>Regulation (EU) 2023/2854 – Data Act</hasLegalSource>
    <hasArticleNumber>4(1)</hasArticleNumber>
    <rdfs:label>Article 4(1) – User Access Rights</rdfs:label>
  </owl:NamedIndividual>

</rdf:RDF>
